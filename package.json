{
  "name": "angular-application",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4201",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run angular-application:serve-ssr",
    "build:prod": "ng build --prod",
    "build:universal": "npm run build:webpack && npm run serve:ssr",
    "build:webpack": "npm run build:ssr && gzip -k -r dist/browser/ && npm run server:webpack",
    "server:webpack": "webpack --config webpack.server.config.js --progress --color",
    "build:ssr": "ng build --prod && npm run build:server",
    "build:server": "ng run angular-application:server:production",
    "serve:ssr": "node dist/server/main.js",
    "serve:browser": "http-server dist/browser",
    "prerender": "ng run angular-application:prerender",
    "bundle-report": "npm run build:ssr && gzip -k -r dist/browser/ && webpack --config webpack.bundle-stats.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.13",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.13",
    "@angular/compiler": "~9.1.13",
    "@angular/core": "~9.1.13",
    "@angular/forms": "~9.1.13",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.13",
    "@angular/platform-browser-dynamic": "~9.1.13",
    "@angular/platform-server": "~9.1.13",
    "@angular/router": "~9.1.13",
    "@angular/service-worker": "~9.1.13",
    "@nguniversal/express-engine": "^9.1.1",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.18.0",
    "express": "^4.15.2",
    "http-server": "^0.12.3",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular/cli": "~9.1.15",
    "@angular/compiler-cli": "~9.1.13",
    "@nguniversal/builders": "^9.1.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "eslint": "^8.2.0",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-loader": "^8.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
